        <value>var kosten = current.betrag;&#13;
var antrag = new GlideAggregate("x_526619_reisekost_g7kosten");&#13;
antrag.addQuery('antrag', current.antrag.sys_id);&#13;
antrag.addQuery('genehmigt', "2");&#13;
antrag.groupBy('antrag');&#13;
antrag.addAggregate('SUM', 'betrag');&#13;
antrag.query();&#13;
&#13;
&#13;
if (antrag.next()) {&#13;
    kosten += parseFloat(antrag.getAggregate('SUM', 'betrag'));&#13;
}&#13;
&#13;
var endab = new GlideRecord("x_526619_reisekost_endabrechnung");&#13;
endab.addQuery("antrag", current.antrag.sys_id);&#13;
endab.query();&#13;
if (endab.next()) {&#13;
    endab.setValue("endbetrag", kosten);&#13;
    endab.update();&#13;
    gs.addInfoMessage("Der Betrag in Höhe von " + current.betrag + " wurde der Endabrechnung hinzugefügt.");&#13;
} else {&#13;
    gs.addErrorMessage("Es ist leider etwas schiefgelaufen");&#13;
}</value>
